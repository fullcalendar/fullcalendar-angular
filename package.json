{
  "private": true,
  "name": "fullcalendar-angular-workspace",
  "version": "5.10.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:meta && ng build fullcalendar --configuration production",
    "build:meta": "mkdir -p dist/fullcalendar && cp LICENSE.* README.* dist/fullcalendar",
    "build:demo": "ng build fullcalendar-demo --configuration='production'",
    "test": "ng test fullcalendar --watch=false --browsers ChromeHeadless",
    "test:watch": "ng test fullcalendar --watch=true",
    "test:demo": "ng test",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "clean": "./scripts/clean.sh",
    "ci": "./scripts/ci.sh",
    "sync-version-fields": "./scripts/sync-version-fields.js",
    "version": "npm run sync-version-fields && git add projects/fullcalendar/package.json"
  },
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@fullcalendar/core": "~5.10.1",
    "@fullcalendar/daygrid": "~5.10.1",
    "@fullcalendar/interaction": "~5.10.1",
    "@fullcalendar/timegrid": "~5.10.1",
    "core-js": "^3.6.2",
    "fast-deep-equal": "^3.1.1",
    "rxjs": "^7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.1",
    "@angular/cli": "~13.1.1",
    "@angular/compiler-cli": "~13.1.0",
    "@angular/language-service": "~13.1.0",
    "@types/jasmine": "^3.10.2",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^14.18.0",
    "codelyzer": "^6.0.0",
    "fullcalendar-scheduler": "~5.10.1",
    "jasmine-core": "^3.10.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^13.1.1",
    "protractor": "~7.0.0",
    "ts-node": "^10.4.0",
    "tslint": "~6.1.0",
    "typescript": "4.5.3"
  }
}
