{
  "name": "fullcalendar-angular",
  "version": "4.1.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:demo && npm run build:lib",
    "build:demo": "ng build fullcalendar-demo",
    "build:lib": "ng build fullcalendar && npm run build:lib:meta",
    "build:lib:meta": "cp LICENSE.* README.* dist/fullcalendar",
    "test": "ng test",
    "test:lib": "ng test fullcalendar --browsers ChromeHeadless --watch=false",
    "test:lib:watch": "ng test fullcalendar --watch=true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ci": "npm run e2e && npm run test:lib && npm run build",
    "clean": "rm -rf dist",
    "preversion": "npm run clean",
    "version": "./scripts/sync-version-fields && npm run e2e && npm run build:lib && git add -f package-lock.json dist/fullcalendar"
  },
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "@fullcalendar/core": "^4.1.0",
    "@fullcalendar/daygrid": "^4.1.0",
    "@fullcalendar/interaction": "^4.1.0",
    "core-js": "^2.5.4",
    "deep-copy": "^1.4.2",
    "fast-deep-equal": "^2.0.1",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "~7.0.5",
    "@angular/compiler-cli": "^7.2.12",
    "@angular/language-service": "~7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
